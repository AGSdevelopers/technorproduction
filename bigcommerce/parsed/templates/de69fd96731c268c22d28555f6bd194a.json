{"components/common/header":"<div class=\"header-container {{#if theme_settings.sticky_topMenu '===' true}}sticky-header{{/if}}\">\r\n\r\n    <div class=\"top-ribbon\">\r\n        <div class=\"content-ribbon\">\r\n            <div class=\" account-links alm\" style=\"display: none;\">\r\n                <ul>\r\n                    {{#if settings.account_creation_enabled}}\r\n                        <li class=\"navUser-item navUser-item--account\" style=\"margin-right: 2rem;\">\r\n                            {{#if customer}}\r\n                                <a class=\"navUser-action account\" href=\"{{urls.account.index}}\"><i class=\"fa fa-user\" aria-hidden=\"true\"></i></a>\r\n                                <a class=\"navUser-action logout\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n                            {{else}}\r\n                                <a class=\"navUser-action\" href=\"{{urls.auth.login}}\" ><!-- <i class=\"fa fa-user\" aria-hidden=\"true\"></i> --><a class=\"navUser-action login\" href=\"{{urls.auth.login}}\" style=\"padding-right: 15px;border-right: 1px solid;\">{{lang 'common.login'}}</a><span class=\"or-text\">|</span><a class=\"navUser-action create\" href=\"{{urls.auth.create_account}}\">{{lang 'Sign Up'}}</a>\r\n                            {{/if}}\r\n                         </li>\r\n                    {{/if}}\r\n                    <!-- <li class=\"navUser-item navUser-item--cart\">\r\n                        <a\r\n                            class=\"navUser-action\"\r\n                            data-cart-preview\r\n                            data-dropdown=\"cart-preview-dropdown\"\r\n                            data-options=\"align:right\"\r\n                            href=\"{{urls.cart}}\">\r\n                            <span class=\"navUser-item-cartLabel\"><i class=\"fa fa-shopping-cart\" aria-hidden=\"true\"></i> <i class=\"fa-solid fa-cart-shopping\"></i> </span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                        </a>\r\n\r\n                        <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n                    </li> -->\r\n                </ul>\r\n            </div>\r\n            <span>SAME DAY shipping when you order by 2pm CT (M-Sa).  ðŸš› FREE shipping on all US continental orders.</span>\r\n            <div class=\"right account-links ald\">\r\n                <ul>\r\n                    {{#if settings.account_creation_enabled}}\r\n                        <li class=\"navUser-item navUser-item--account\" style=\"margin-right: 2rem;\">\r\n                            {{#if customer}}\r\n                                <a class=\"navUser-action account\" href=\"{{urls.account.index}}\"><i class=\"fa fa-user\" aria-hidden=\"true\"></i></a>\r\n                                <a class=\"navUser-action logout\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n                            {{else}}\r\n                                <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"><!-- <i class=\"fa fa-user\" aria-hidden=\"true\"></i> --><a class=\"navUser-action login\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a><span class=\"or-text\">|</span><a class=\"navUser-action create\" href=\"{{urls.auth.create_account}}\">{{lang 'Sign Up'}}</a>\r\n                            {{/if}}\r\n                         </li>\r\n                    {{/if}}\r\n                    <li class=\"navUser-item navUser-item--cart\">\r\n                        <a\r\n                            class=\"navUser-action\"\r\n                            data-cart-preview\r\n                            data-dropdown=\"cart-preview-dropdown\"\r\n                            data-options=\"align:right\"\r\n                            href=\"{{urls.cart}}\">\r\n                            <span class=\"navUser-item-cartLabel\"><i class=\"fa fa-shopping-cart\" aria-hidden=\"true\"></i> <i class=\"fa-solid fa-cart-shopping\"></i> </span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                        </a>\r\n\r\n                        <div class=\"dropdown-menu ddmm\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n                    </li>\r\n                </ul>\r\n            </div> \r\n        </div>\r\n    </div>\r\n\r\n    <div class=\"header-container full-width {{#if theme_settings.slider_width '===' 'full-width'}} full-width-auto {{/if}} full-width{{#if theme_settings.header-layout '!==' 'full-width'}} in-container{{/if}}\">\r\n      <div class=\"header-padding{{#if theme_settings.header-layout '===' 'in-container-with-padding-no-margin'}} in-container-with-padding-no-margin{{/if}}\">\r\n      <div class=\"header-padding{{#if theme_settings.header-layout '===' 'in-container-with-padding'}} in-container-with-padding{{/if}}\">\r\n      <header class=\"header\" role=\"banner\">\r\n\r\n        <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n        </a>\r\n            <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                {{> components/common/store-logo}}\r\n            </h1>\r\n            <!-- <li class=\"navUser-item  navUser-item--cart\">\r\n                <a\r\n                    class=\"navUser-action\"\r\n                    data-cart-preview\r\n                    data-dropdown=\"cart-preview-dropdown\"\r\n                    data-options=\"align:right\"\r\n                    href=\"{{urls.cart}}\">\r\n                    <span class=\"navUser-item-cartLabel\"><i class=\"fa fa-shopping-cart\" aria-hidden=\"true\"></i></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                </a>\r\n\r\n                <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n            </li> -->\r\n\r\n\r\n            <div class=\"navPages-container main-nav main-nav-custom\" id=\"menu\" data-menu>\r\n                <nav class=\"navPages\">\r\n                    <div class=\"custom-pages-nav\">\r\n                        <div id=\"desktop-menu\" class=\"custom-menu-desktop\">\r\n                        <h2 class=\"container-header mobile\">Main Menu</h2>\r\n                        {{#each pages}}\r\n                            <li class=\"navPages-item\">\r\n                                {{> components/common/navigation-list}}\r\n                            </li>\r\n                        {{/each}}\r\n                        </div>\r\n                    </div>\r\n                </nav>\r\n            </div>\r\n\r\n\r\n\r\n\r\n    <!-- Begin Search Bar -->\r\n\r\n\r\n\r\n     <div class=\"navPages-container inside-header left-logo\" id=\"menu\" data-menu>\r\n        <nav class=\"navPages\">\r\n            <div class=\"navPages-quickSearch right searchbar\">\r\n                {{> components/common/quick-search}}\r\n            </div>\r\n        </nav>\r\n    </div>\r\n    <div class=\"navPages-container inside-header right-logo\" id=\"menu\" data-menu>\r\n        <nav class=\"navPages\">\r\n            <div class=\"navPages-quickSearch right searchbar\">\r\n                {{> components/common/quick-search}}\r\n            </div>\r\n        </nav>\r\n    </div>\r\n    <div class=\"navPages-container inside-header center-logo\" id=\"menu\" data-menu>\r\n        <nav class=\"navPages\">\r\n            <div class=\"navPages-quickSearch right searchbar\">\r\n                {{> components/common/quick-search}}\r\n            </div>\r\n        </nav>\r\n    </div>\r\n\r\n\r\n\r\n    <!-- End Search Bar -->\r\n\r\n\r\n\r\n\r\n    <div class=\"mobile-menu navPages-container\" id=\"menu\" data-menu>\r\n      <div class=\"currency-converter\">\r\n        <ul class=\"navPages-list navPages-list--user\">\r\n            {{#if currency_selector.currencies.length '>' 1}}\r\n                <li class=\"navPages-item\">\r\n                    <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\r\n                        {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\r\n                    </a>\r\n                    <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                        <ul class=\"navPnavPagesage-subMenu-list\">\r\n                            {{#each currency_selector.currencies}}\r\n                            <li class=\"navPage-subMenu-item\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                    {{#if is_active}}\r\n                                        <strong>{{name}}</strong>\r\n                                    {{else}}\r\n                                        {{name}}\r\n                                    {{/if}}\r\n                                </a>\r\n                            </li>\r\n                            {{/each}}\r\n                        </ul>\r\n                    </div>\r\n                </li>\r\n            {{/if}}\r\n        </ul>\r\n      </div>\r\n            {{> components/common/navigation-menu}}\r\n    </div>\r\n    </header>\r\n    </div>\r\n    </div>\r\n    </div>\r\n\r\n    <div class=\"main-nav-container{{#if theme_settings.header-layout '!==' 'in-container'}} full-width{{/if}} {{#if theme_settings.header-layout '===' 'in-container'}} in-container{{/if}}\">\r\n      <div class=\"navPages-container main-nav\" id=\"menu\" data-menu>\r\n            {{> components/common/navigation-menu}}\r\n      </div>\r\n    </div>\r\n    </div>\r\n    <div>\r\n\r\n        <div class=\"navPages-quickSearch searchbar\" >\r\n            {{> components/common/quick-search}}\r\n        </div>\r\n\r\n    </div>\r\n","components/common/navigation-list":"{{#if children}}\n<a class=\"navPages-action has-subMenu\" href=\"{{url}}\">\n    {{name}} <i class=\"fa fa-chevron-down main-nav\" aria-hidden=\"true\"></i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\n                        href=\"{{url}}\"\n                        data-collapsible=\"navPages-{{id}}\"\n                        data-collapsible-disabled-breakpoint=\"medium\"\n                        data-collapsible-disabled-state=\"open\"\n                        data-collapsible-enabled-state=\"closed\">\n                        {{name}} <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n","components/common/navigation-menu":"<nav class=\"navPages\">\n    <ul class=\"navPages-list\">\n        <div class=\"nav-align{{#if theme_settings.mainNav-align '!==' 'left'}} center{{/if}}\">\n        <div class=\"custom-pages-nav\">\n          <div id=\"desktop-menu\">\n            <h2 class=\"container-header mobile\">Main Menu</h2>\n            {{#each pages}}\n                <li class=\"navPages-item\">\n                    {{> components/common/navigation-list}}\n                </li>\n            {{/each}}\n          </div>\n            <div id=\"mobile-menu\">\n              <div class=\"category-menu\">\n                <h2 class=\"container-header mobile\">Shop By Category</h2>\n                {{#each categories}}\n                  <li class=\"navPages-item mobile\">\n                    {{> components/common/navigation-list}}\n                  </li>\n                {{/each}}\n              </div>\n\n            {{#if theme_settings.brands_visibility_mobile_menu '===' true}}\n            <div class=\"brand-menu\">\n              <h2 class=\"container-header mobile\">Shop By Brand</h2>\n              {{#each shop_by_brand}}\n                <li class=\"navPages-item mobile\">\n                  <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n                </li>\n              {{/each}}\n                  <li class=\"navPages-item mobile\">\n                    <a class=\"navPages-action\" href=\"/brands/\">View all Brands</a>\n                  </li>\n            <div class=\"clear\"></div>\n            <div class=\"navPages-quickSearch right searchbar\">\n                {{> components/common/quick-search}}\n            </div>\n          </div>\n          {{/if}}\n\n          </div>\n        </div>\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPnavPagesage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n        </li>\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item account\">\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    {{lang 'common.account'}} <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</span></a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            </li>\n        {{else}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> {{lang 'common.or'}} <a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n","components/common/quick-search":"<div class=\"container\">\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"u-hiddenVisually\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n                <div class=\"search-icon\"><i class=\"fa fa-search\" aria-hidden=\"true\"></i></div>\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">\n            </div>\n        </fieldset>\n        <div class=\"clear\"></div>\n    </form>\n      <a href=\"#\" class=\"reset quicksearch\" aria-label=\"reset search\" role=\"button\">\n        <span class=\"reset\" aria-hidden=\"true\">Reset Search</span>\n      </a>\n      <a href=\"#\" class=\"modal-close custom-quick-search\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n      </a>\n      <section class=\"quickSearchResults\" data-bind=\"html: results\">\n      </section>\n</div>\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"}
